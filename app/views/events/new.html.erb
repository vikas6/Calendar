<div class="main-content">
	<div class="form">
		<h1>Event Add</h1>
    <%= form_tag event_create_path do %>
      <p><%= text_field_tag :title, nil, placeholder: 'Event title...',:required => true %></p>
       <p><%= label_tag :when %></p>
       <p><%= time_tag Time.now  %></p>
      <button class="event_create">Create</button>
      <button class="event_edit"> <%= link_to "Edit", event_edit_path %></button>
    <% end %>
	</div>
</div>

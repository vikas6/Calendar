<!DOCTYPE html>
<html>
<head>
  <title>Calendar</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="header">
  <div class="header">
    <h1>GrepRuby Event Calendar</h1>
  </div>
  <% if current_user %>
  <div class="user_nav">
    <h2> Logged in as</h2><h3><%= current_user.username %><%= link_to "Log out", log_out_path %></h3>
      <% end %>
  </div>
</div>
<%= yield %>
 
 
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>

</body>
</html>
